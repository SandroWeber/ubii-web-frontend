.tile-menu-list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-content: center;
  margin: 10px;
}

.tile-menu-item {
  width: 125px;
  height: 125px;
  text-align: center;
  cursor: pointer;
}

.tile-menu-item-background {
  width: 100%;
  height: 100%;
}

.tile-menu-icon {
  margin: 10px;
  width: 25px;
  height: 25px;
}

.tile-menu-category {}

.tile-menu-category-seperator {
  border-bottom: solid 1px highContrastColor;
  height: 10px;
  line-height: 20px;
  text-align: left;
  margin: 15px;
}

.tile-menu-category-seperator-label {
  display: inline;
  padding-left: 15px;
  padding-right: 20px;
  background-color: layerOneSecondaryColor;
}
